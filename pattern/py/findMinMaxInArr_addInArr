def pipe_fix(nums):
    result = []
    rMin = nums[0]
    rMax = nums[0]
    
    for item in nums:
        if item < rMin :
            rMin = item
        elif item > rMax:
            rMax = item
    
    for r in range(rMin, rMax + 1):
        result.append(r)
    
    return result

def pipe_fix(nums):
	return list(range(nums[0], nums[-1] + 1))
